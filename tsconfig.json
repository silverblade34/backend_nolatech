{
    "compilerOptions": {
      "target": "es2016",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "typeRoots": ["./types", "./node_modules/@types"],
      "module": "commonjs",
      "esModuleInterop": true,
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist"
    }
}